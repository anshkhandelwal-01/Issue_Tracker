<link rel="stylesheet" href="/css/home.css">
<div id="main-body">
  <h1>Welcome to Bug Locater!</h1>
  <h2>List of Projects</h2>
  <ol>
    <% projects.forEach((project) => { %>
      <li>
        <a href="/projects/<%= project._id %>">
          <%= project.name %>
        </a>
      </li>
    <% }) %>
    </ol>
  <a href="/projects/create"><button>Create New Project</button></a>
</div>